    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.123",
    "@mui/material": "^5.11.13",
    "@mui/styled-engine-sc": "^5.11.11",
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint": "^8.52.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
